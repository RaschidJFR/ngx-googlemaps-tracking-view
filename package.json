{
  "name": "googlemaps-tracking-view-demo",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve --vendorSourceMap",
    "build": "ng build ngx-googlemaps-tracking-view",
    "build:demo": "cd demo && ng build --base-href=\".\"",
    "compodoc": "npx compodoc -p ./projects/ngx-googlemaps-tracking-view/tsconfig.lib.json -d dist/demo/www/reference",
    "predeploy": "rm -rf dist && ng build ngx-googlemaps-tracking-view && npm run link:library",
    "deploy": "npm run build:demo && npm run compodoc && cp -R demo/www dist/demo && npx ngh --dir=\"dist/demo/www\"",
    "prepublish": "rm -rf dist",
    "publish": "npm run build && cp README.md dist/ngx-googlemaps-tracking-view && npm publish ./dist/ngx-googlemaps-tracking-view",
    "install:library": "cd projects/ngx-googlemaps-tracking-view && npm install",
    "link:library": "cd dist/ngx-googlemaps-tracking-view && npm link && cd ../.. && npm link ngx-googlemaps-tracking-view",
    "postinstall": "npm run install:library && npm run build && npm run link:library",
    "watch": "ng build ngx-googlemaps-tracking-view --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "angular-cli-ghpages": "^0.5.3",
    "core-js": "^2.5.4",
    "rxjs": "~6.6.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.29",
    "@angular-devkit/build-ng-packagr": "~0.803.29",
    "@angular/cli": "~8.3.29",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/googlemaps": "^3.37.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.7.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslib": "^1.14.1",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  }
}
